/*================ Contact Section =================*/
.contact-section{background: url(../images/contact-bg.png) repeat-y center top / cover; background-size:cover; padding:3rem 0; margin-top:-140px;}
.form-control{ border-radius:var(--b-radius15) !important; height:46px; background:var(--white); font-weight:500; padding:5px 12px !important; }
textarea.form-control{height:110px; resize:none;}
.contact-section select.form-control{appearance: none;
    background: url(../images/arrow_blue.png) no-repeat var(--white);
    background-position: right 0rem center;
    background-size: 3em;}
.contact-section .col-md-12:nth-child(5) textarea.form-control{height:100px; }
.contact-section .col-md-12:nth-child(6) textarea.form-control{height:150px; }
.wpcf7-spinner{display: none;}
/*================ Footer Section =================*/
.site-footer{ background:var(--black); padding:25px 0; }
.site-footer a{ color:var(--white); }
.site-footer a:hover{ color:var(--gray); }
.social-block a{ font-size:25px; }

/*================ Popup Section =================*/
.modal-content{border-radius: var(--b-radius50);}
.modal ul{ margin:0; }
.modal ul li{list-style-type: disc; margin-bottom: 5px;}
.home .modal .btn-close{font-size:18px; color:var(--black); opacity:1; top: 0px; position: relative; margin: 0; left:25px;}
.home .modal .close-btn{
	cursor:pointer;
}
.modal .btn-close:hover{opacity:0.6; }
.modal h3{ font-weight:700; }
.modal{color:var(--black);}
